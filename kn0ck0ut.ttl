@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix ev: <http://lv2plug.in/ns/ext/event#>.
<urn:malte.steiner:plugins:minicomputer> a lv2:Plugin ;
    doap:name "Kn0ck0ut Vocal Remover" ;
    doap:developer [
      foaf:name "St3pan0va";
    ] ;
    doap maintainer [ 
      foaf:name "Jeremy Salwen";
      foaf:mbox <mailto:jeremysalwen@gmail.com>;
    ] ;
    
    lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "left" ;
    lv2:name "Input Left" ;
    ], [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "right" ;
    lv2:name "Input Right" ;
    ],[
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "out" ;
    lv2:name "Output" ;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "mode";
    lv2:name "Mode";
    lv2:portProperty lv2:toggled;
    lv2:portProperty lv2:enumeration;
    lv2:default 0;
    lv2:scalePoint [ rdfs:label "Extract Centre"; rdf:value 1 ];
    lv2:scalePoint [ rdfs:label "Remove Centre"; rdf:value 0 ];
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "lowcut";
    lv2:name "Low Frequency Cut";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 128;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "highcut";
    lv2:name "High Frequency Cut";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "decay";
    lv2:name "Decay";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 127;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "blur";
    lv2:name "Blur";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 24;
    ].


ports:

in 1,
in 2,
out
kcentre boolean "Extract centre"
locut float "Lo freq cut"
hicut float "Hi freq cut"
decay float "Decay"
blur float "Blur"

units:
		case kCentre :   strcpy (label, "        "); 	break;
		case kIn :		strcpy (label, "   dB   ");	break;		
		case kLoCut : strcpy (label, " threshold ");	break;
		case kOut :      strcpy (label, "   dB   ");	break;
		case kHiCut : strcpy (label, " threshold ");	break;
		case kDecay : strcpy(label, " amount "); break;
		case kBlur : strcpy(label, " amount "); break;
		
		default values: 	fCentre = 0;
	fLoCut = 0;
	fHiCut = 0;
	fDecay = 0;