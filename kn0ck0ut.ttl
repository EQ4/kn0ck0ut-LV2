@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix ev: <http://lv2plug.in/ns/ext/event#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<urn:St3pan0va.plugins.kn0ck0ut> a lv2:Plugin ;
    doap:name "Kn0ck0ut Vocal Remover" ;
    lv2:binary <libkn0ck0ut.so>;
    doap:developer [
      foaf:name "St3pan0va";
    ] ;
    doap:maintainer [ 
      foaf:name "Jeremy Salwen";
      foaf:mbox <mailto:jeremysalwen@gmail.com>;
    ] ;
    ll:pegName "p";
    lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "left" ;
    lv2:name "Input Left" ;
    ], [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "right" ;
    lv2:name "Input Right" ;
    ],[
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "out" ;
    lv2:name "Output" ;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 3;
    lv2:symbol "mode";
    lv2:name "Mode";
    lv2:portProperty lv2:toggled;
    lv2:portProperty lv2:enumeration;
    lv2:default 0;
    lv2:scalePoint [ rdfs:label "Extract Centre"; rdf:value 1 ];
    lv2:scalePoint [ rdfs:label "Remove Centre"; rdf:value 0 ];
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "lowcut";
    lv2:name "Low Frequency Cut";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 128;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 5;
    lv2:symbol "highcut";
    lv2:name "High Frequency Cut";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 6;
    lv2:symbol "decay";
    lv2:name "Decay";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 127;
    ],[
    a lv2:InputPort;
    a lv2:ControlPort;
    lv2:index 7;
    lv2:symbol "blur";
    lv2:name "Blur";
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 24;
    ].
